class Point:                      ##construction of point class
  def __init__(self, x, y, z):    ##implementation of constructor
    self.x = x
    self.y = y
    self.z = z

  def sqSum(self):
    a = self.x
    b = self.y
    c = self.z
    x = a*a
    y = b*b
    z = c*c

    sum = f'SQUARED SUM OF GIVEN NUMBERS IS: {x+y+z}'
    return sum

obj = Point(1,3,5)

print(obj.sqSum())
